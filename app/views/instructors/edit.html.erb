<%= form_for @instructor do |f| %>
    <%= f.label :name, 'Nome do Instrutor' %>
    <%= f.text_field :name %>
    <%= f.label :email, 'Email' %>
    <%= f.text_field :email %>
    <%= f.label :bio, 'Bio' %>
    <%= f.text_field :bio %>
    <%= f.label :profile_picture, "Foto" %>
    <%= f.file_field :profile_picture %>
    <%= f.submit 'Atualizar' %>
<% end %>